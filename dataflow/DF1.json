{
	"name": "DF1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Assets",
						"type": "DatasetReference"
					},
					"name": "Assets"
				},
				{
					"dataset": {
						"referenceName": "KEV",
						"type": "DatasetReference"
					},
					"name": "KEV"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "alertquery"
				}
			],
			"scriptLines": [
				"source(output(",
				"          application as string,",
				"          ip as string,",
				"          hostname as string,",
				"          dep as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Assets",
				"source(output(",
				"          {CVE ID} as string 'var',",
				"          {Vendor Project} as string,",
				"          Product as string,",
				"          {Vulnerability Name} as string,",
				"          {Date Added} as date,",
				"          {Short Description} as string,",
				"          {Required Action} as string,",
				"          {Due Date} as string,",
				"          Notes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> KEV",
				"Assets, KEV join(application == Product,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> alertquery"
			]
		}
	}
}