{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "assets"
				},
				{
					"dataset": {
						"referenceName": "KEV",
						"type": "DatasetReference"
					},
					"name": "KEV"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "lookup1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> assets",
				"source(output(",
				"          {CVE ID} as string,",
				"          {Vendor Project} as string,",
				"          Product as string,",
				"          {Vulnerability Name} as string,",
				"          {Date Added} as string,",
				"          {Short Description} as string,",
				"          {Required Action} as string,",
				"          {Due Date} as string,",
				"          Notes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> KEV",
				"assets, KEV lookup(application == Product,",
				"     multiple: true,",
				"     broadcast: 'auto')~> lookup1"
			]
		}
	}
}