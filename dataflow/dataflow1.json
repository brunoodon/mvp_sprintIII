{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "assets_http",
						"type": "DatasetReference"
					},
					"name": "assets"
				},
				{
					"dataset": {
						"referenceName": "kev_http",
						"type": "DatasetReference"
					},
					"name": "KEV"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "Alerts"
				}
			],
			"scriptLines": [
				"source(output(",
				"          application as string,",
				"          ip as string,",
				"          hostname as string,",
				"          dep as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> assets",
				"source(output(",
				"          Vulnerability as string,",
				"          SeverityScore as string,",
				"          VulnerabilityName as string,",
				"          AlertDate as string,",
				"          Application as string,",
				"          Department as string,",
				"          Description as string,",
				"          ExploitationVector as string,",
				"          Hostname as string,",
				"          IP as string,",
				"          Vendor as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> KEV",
				"assets, KEV lookup(assets@application == KEV@Application,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> Alerts"
			]
		}
	}
}